{
  "name": "better-artists",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "spicetify-creator",
    "build-local": "spicetify-creator --out=../better-artists --minify",
    "watch": "spicetify-creator --watch",
    "w:local": "spicetify-creator --watch --out=../better-artists --minify",
    "w:tailwind": "tailwindcss -i ./src/css/input.css -o ./src/css/output.css --watch"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "spicetify-creator": "^1.0.17",
    "tailwindcss": "^3.4.7"
  },
  "dependencies": {
    "tippy.js": "^6.3.7",
    "usehooks-ts": "^3.1.0"
  }
}
